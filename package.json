    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.18.3",
    "multer": "^2.0.2",
    "puppeteer": "^24.34.0",
